<% content_for :title do %>
	<%= " - Contact Us"%>
<% end %>
<% content_for :og_tags do %>
	<meta property="og:title" content="Spa911Carwash - Contact Us"/>
	<meta property="og:description" content="Eco-friendly exterior body wash, Interior clean, Tyre dressing, Windshield cleaning, Dry Wash, Car Wash, Carwash in pune, Car waxing, Car polishing, Car Detail Cleaining "/>
	<meta property="og:url" content="<%= contact_us_url %>" />
	<meta property="og:image" content="" />
<% end %>
<aside id="fh5co-hero">
	<div class="flexslider">
		<ul class="slides">
	   	<li style="background-image: url(assets/slide_3.jpg);">
	   		<div class="overlay-gradient"></div>
	   		<div class="container">
	   			<div class="col-md-10 col-md-offset-1 text-center js-fullheight slider-text">
	   				<div class="slider-text-inner">
	   					<h1></h1>
	   					<p>&nbsp;</p>
	   				</div>
	   			</div>
	   		</div>
	   	</li>
  	</ul>
	</div>
</aside>

<div class="fh5co-contact">
	<div class="container" id="nav-services">
		<div class="row">
			<div class="col-md-3 animate-box">
				<h3>Contact Info.</h3>
				<ul class="contact-info">
					<li><i class="icon-map"></i>Pimpri Chinchwad Pune Maharashtra</li>
					<li><i class="icon-phone"></i>+91 8806097911</li>
					<li><i class="icon-phone"></i>+91 7768007911</li>
					<li><i class="icon-envelope"></i><%= mail_to "spa911carwash@gmail.com"%></li>
					<li><i class="icon-globe"></i><%= link_to "www.spa911carwash.in", "http://www.spa911carwash.in", target: "_blank?"%></li>
				</ul>
			</div>
			<%= form_tag contact_us_spa_emails_path do %>
				<div class="col-md-8 col-md-push-1 col-sm-12 col-sm-push-0 col-xs-12 col-xs-push-0">
					<div class="row">
						<div class="col-md-6">
							<div class="form-group">
								<%= text_field_tag :name, nil, class: 'form-control', placeholder: 'Your Name here' %>
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<%= text_field_tag :email, nil, class: 'form-control', placeholder: 'Your Email here' %>
								
							</div>
						</div>
						<div class="col-md-12">
							<div class="form-group">
								<%= text_area_tag :message,nil,class: "form-control",cols: "30", rows: "7", placeholder: "Message"   %>
							</div>
						</div>
						<div class="col-md-12">
							<div class="form-group">
								<%= submit_tag "Send Message", class: "btn btn-primary" %>
							</div>
						</div>
					</div>
				</div>
			<% end %>
		</div>
	</div>	
</div>

<div id="map" class="animate-box" data-animate-effect="fadeIn">
	<!----- contact-grids ----->		
		<div class="contact-grids">
			<div class="contact-map">
				<iframe width="100%" height="300" src="http://www.maps.ie/create-google-map/map.php?width=100%&amp;height=300&amp;hl=en&amp;q=wakad%20+(SPA%20911%20for%20Car)&amp;ie=UTF8&amp;t=&amp;z=13&amp;iwloc=A&amp;output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"><a href="http://www.mapsdirections.info/de/erstellen-sie-eine-google-map/">Google Maps karte einbinden</a> by <a href="http://www.mapsdirections.info/de/">Erstellen Google Map</a></iframe>
			</div>
			<!----- contact-form ------>
</div>